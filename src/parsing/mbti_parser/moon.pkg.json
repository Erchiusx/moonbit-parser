{
  "pre-build": [
    {
      "command": "$mod_dir/.mooncakes/moonbitlang/yacc/moonyacc --external-tokens --input-mode array $input -o $output && moonfmt -w $output && moonfmt -w $output",
      "input": "mbti_parser.mbty",
      "output": "mbti_parser.mbt"
    }
  ],
  "import": [
    "moonbitlang/parser/lexing/tokens",
    {
      "path": "moonbitlang/parser/parsing/syntax_types",
      "alias": "ast_types"
    },
    "moonbitlang/parser/parsing/mbti_ast",
    "moonbitlang/parser/parsing/syntax"
  ],
  "warn-list": "-6",
  "test-import": [
    "moonbitlang/x/fs",
    "moonbitlang/parser/lexing/lexer"
  ],
  "alert-list": "-deprecated"
}
