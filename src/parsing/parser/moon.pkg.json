{
  "import": [
    "moonbitlang/parser/parsing/syntax_types",
    "moonbitlang/parser/parsing/syntax",
    "moonbitlang/parser/parsing/compact",
    "moonbitlang/parser/lexing/tokens"
  ],
  "pre-build": [
    // {
    //   "command": "$mod_dir/.mooncakes/moonbitlang/yacc/moonyacc --mode only-tokens $input -o $output",
    //   "input": "parser.mbty",
    //   "output": "tokens.mbt"
    // },
    // {
    //   "command": "$mod_dir/.mooncakes/moonbitlang/ulex/moonlex --code-unit 1 --default-encoding utf8 $input -o $output",
    //   "input": "lexer.mbtx",
    //   "output": "lexer.mbt"
    // }
    {
      "command": "$mod_dir/.mooncakes/moonbitlang/yacc/moonyacc --no-comments --external-tokens $input -o $output && moonfmt -w $output && moonfmt -w $output",
      "input": "parser.mbty",
      "output": "parser.mbt"
    }
  ],
  "warn-list": "-6",
  "alert-list": "-deprecated"
}
