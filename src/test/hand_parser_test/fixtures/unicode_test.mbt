
enum 类型 {
    T(Int)
}

fn 斐波那契数列(第几个数: Int) -> Int {
    let 内部循环 = fn(前一个, 后一个, 计数器) {
        if 计数器 >= 第几个数 {
            前一个
        } else {
            内部循环(后一个, 前一个 + 后一个, 计数器 + 1)
        }
    }
}

fn[数字类型] 相加(左: 数字类型, 右: 数字类型) -> 数字类型 {
    左 + 右
}

fn[T] 打印(self: T) {
    println(self)
}

fn init {
    let 答案 = 斐波那契数列(20)
    相加(答案, 42).打印()
}
