{
  "import": [
    "moonbitlang/parser/syntax",
    "moonbitlang/parser/syntax/util",
    "moonbitlang/parser/tokens",
    "moonbitlang/parser/basic",
    "moonbitlang/parser/attribute"
  ],
  "pre-build": [
    {
      "command": "$mod_dir/.mooncakes/moonbitlang/yacc/moonyacc --no-comments --external-tokens $input -o $output && moonfmt -w $output && moonfmt -w $output",
      "input": "parser.mbty",
      "output": "parser.mbt"
    }
  ],
  "warn-list": "-6"
}